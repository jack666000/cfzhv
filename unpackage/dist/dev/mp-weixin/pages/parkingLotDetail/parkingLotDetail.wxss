/* 水平间距 */
page {
  background: #F5F7FF;
  padding-bottom: 200rpx;
}
.bottomBox {
  width: 750rpx;
  height: 165rpx;
  background: #FFFFFF;
  box-shadow: 0rpx -12rpx 14rpx 0rpx rgba(222, 220, 238, 0.5);
  position: fixed;
  left: 0;
  bottom: 0;
  padding: 0 32rpx;
  overflow: hidden;
  padding-bottom: 20rpx;
}
.bottomBox .left {
  width: 332rpx;
  height: 100rpx;
  background: rgba(3, 148, 246, 0.18);
  border-radius: 10rpx;
}
.bottomBox .left image {
  width: 40rpx;
  height: 40rpx;
}
.bottomBox .left .text {
  height: 50rpx;
  font-size: 36rpx;
  font-weight: 600;
  color: #6794F6;
  line-height: 50rpx;
  margin-left: 12rpx;
}
.bottomBox .right {
  width: 332rpx;
  height: 100rpx;
  background: #6794F6;
  border-radius: 10rpx;
}
.bottomBox .right image {
  width: 40rpx;
  height: 40rpx;
}
.bottomBox .right .text {
  height: 50rpx;
  font-size: 36rpx;
  font-weight: 600;
  color: #FFFFFF;
  line-height: 50rpx;
  margin-left: 12rpx;
}
.imageBox {
  width: 686rpx;
  height: 216rpx;
  background: #FFFFFF;
  border-radius: 10rpx;
  margin-left: 32rpx;
}
.feeBox {
  width: 686rpx;
  background: #FFFFFF;
  border-radius: 10rpx;
  margin: 32rpx 0 0 32rpx;
  padding: 32rpx;
}
.feeBox .feeBox_item2 {
  width: 622rpx;
  height: 70rpx;
  background: #F8F8F8;
  border-radius: 8rpx;
  margin-top: 28rpx;
  padding: 20rpx 24rpx;
}
.feeBox .feeBox_item1 {
  width: 622rpx;
  height: 64rpx;
  background: rgba(255, 103, 103, 0.15);
  border-radius: 10rpx;
  border: 2rpx solid rgba(255, 103, 103, 0.32);
  margin-top: 24rpx;
  font-size: 24rpx;
  font-weight: 400;
  color: #FF6767;
  line-height: 64rpx;
  padding-left: 26rpx;
}
.feeBox .name {
  height: 50rpx;
  font-size: 36rpx;
  font-weight: 600;
  color: #323233;
  line-height: 50rpx;
}
.scrollViewImage {
  width: 262rpx;
  height: 152rpx;
  border-radius: 16rpx;
  margin: 32rpx 0 0 32rpx;
  display: inline-block;
}
.price {
  margin-top: 24rpx;
}
.price .distance view:nth-child(2) {
  height: 34rpx;
  font-size: 24rpx;
  font-weight: 400;
  color: #434343;
  line-height: 34rpx;
  margin-left: 8rpx;
}
.bgBox {
  width: 750rpx;
  height: 666rpx;
  background: linear-gradient(360deg, #F5F7FF 0%, #A6C1FB 100%);
}
.bgBox .backIcon {
  width: 64rpx;
  height: 64rpx;
  margin: 100rpx 0 0 32rpx;
}
.bgBox .navTitle {
  width: 160rpx;
  height: 56rpx;
  font-size: 40rpx;
  font-weight: 600;
  color: #FFFFFF;
  line-height: 56rpx;
  margin: 100rpx 0 0 190rpx;
}
.bgBox .infoBox {
  width: 686rpx;
  height: 415rpx;
  background: #FFFFFF;
  border-radius: 10rpx;
  margin: 60rpx 0 0 32rpx;
  padding: 32rpx;
}
.bgBox .infoBox .line {
  width: 630rpx;
  opacity: 0.43;
  border: 2rpx dashed #979797;
  margin: 42rpx 0 0 0;
}
.bgBox .infoBox .name {
  height: 50rpx;
  font-size: 36rpx;
  font-weight: 600;
  color: #323233;
  line-height: 50rpx;
}
.bgBox .infoBox .address {
  margin-top: 8rpx;
}
.bgBox .infoBox .address view:nth-child(1) {
  width: 514rpx;
  height: 32rpx;
  font-size: 24rpx;
  font-weight: 400;
  color: #434343;
  line-height: 34rpx;
}
.bgBox .infoBox .spaces {
  height: 32rpx;
  font-size: 24rpx;
  font-weight: 400;
  color: #323233;
  line-height: 32rpx;
  margin-top: 16rpx;
}

