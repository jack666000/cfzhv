/* 水平间距 */
.stopCarList {
  width: 686rpx;
  height: 248rpx;
  background: #FFFFFF;
  border-radius: 16rpx;
  margin: 24rpx 0 0 32rpx;
}
.stopCarList .listTitle .listName {
  width: 484rpx;
  height: 40rpx;
  font-size: 32rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #323233;
  line-height: 40rpx;
  margin: 32rpx 0 0 32rpx;
}
.stopCarList .listTitle .listStatus, .stopCarList .listTitle .listStatus_old {
  width: 128rpx;
  height: 56rpx;
  border-radius: 28rpx;
  border: 2rpx solid #EF3864;
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #EF3864;
  line-height: 56rpx;
  text-align: center;
  margin: 24rpx 0 0 12rpx;
}
.stopCarList .listTitle .listStatus_old {
  border: 2rpx solid #B0B9D0;
  color: #B0B9D0;
}
.stopCarList .licensePlateNumber {
  width: 484rpx;
  height: 40rpx;
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #323233;
  line-height: 40rpx;
  margin: 8rpx 0 0 32rpx;
}
.stopCarList .admissionTime {
  margin-top: 16rpx;
}
.stopCarList .admissionTime .admissionTimeIcon {
  width: 16rpx;
  height: 16rpx;
  background: #0A2463;
  border-radius: 50%;
  margin-left: 32rpx;
}
.stopCarList .appearanceTime {
  margin-top: 16rpx;
}
.stopCarList .appearanceTime .appearanceTimeIcon {
  width: 16rpx;
  height: 16rpx;
  background: #F0AC35;
  border-radius: 50%;
  margin-left: 32rpx;
}
.stopCarList .time {
  width: 590rpx;
  height: 32rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(50, 50, 51, 0.5);
  line-height: 32rpx;
  margin-left: 16rpx;
}
.notBill {
  width: 216rpx;
  height: 34rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(50, 50, 51, 0.7);
  line-height: 34rpx;
  margin-top: 24rpx;
  margin-left: 268rpx;
  text-align: center;
}

