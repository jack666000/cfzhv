  width: 698rpx;
  height: 248rpx;
  background: #FFFFFF;
  border-radius: 10rpx;
  margin: 32rpx 0 0 26rpx;
}
.licensePlateBox .left {
  width: 96rpx;
  font-size: 32rpx;
  font-weight: 400;
  color: rgba(50, 50, 51, 0.6);
  margin-left: 32rpx;
}
.licensePlateBox .right {
  width: 502rpx;
  font-size: 32rpx;
  font-weight: 400;
  color: #323233;
  margin-left: 36rpx;
  text-align: right;
}
