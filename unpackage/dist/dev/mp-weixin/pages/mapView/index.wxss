/* 水平间距 */
map {
  width: 100vw;
  height: 100vh;
}
.myLocal {
  width: 100rpx;
  height: 100rpx;
  background: #FFFFFF;
  box-shadow: 0rpx 8rpx 16rpx 0rpx rgba(0, 0, 0, 0.08), 0rpx 0rpx 2rpx 0rpx rgba(0, 0, 0, 0.08);
  border-radius: 16rpx;
  position: fixed;
  top: 548rpx;
  right: 32rpx;
}
.myLocal image {
  width: 48rpx;
  height: 48rpx;
}
.listcurrently {
  width: 686rpx;
  background: #ffffff;
  border-radius: 10rpx;
  padding: 32rpx;
  position: fixed;
  bottom: 100rpx;
  left: 30rpx;
}
.listcurrently .name {
  height: 50rpx;
  font-size: 36rpx;
  font-weight: 600;
  color: #323233;
  line-height: 50rpx;
}
.listcurrently .address {
  margin-top: 8rpx;
}
.listcurrently .address view:nth-child(1) {
  width: 514rpx;
  height: 32rpx;
  font-size: 24rpx;
  font-weight: 400;
  color: #434343;
  line-height: 34rpx;
}
.listcurrently .spaces {
  height: 32rpx;
  font-size: 24rpx;
  font-weight: 400;
  color: #323233;
  line-height: 32rpx;
  margin-top: 16rpx;
}

