<view><image class="bg" src="../../static/images/charginBg@2x(1).png"></image><view class="scheduleBox flex"><block wx:if="{{show}}"><view class="one">{{$root.m0}}</view></block><view class="two">%</view></view><block wx:if="{{show}}"><view class="estimatedTime">{{"预计还需要\n\t\t"+chargeInfo.contents.chargingedRemainingTime/60000+'分钟 完成'}}</view></block><view class="ChargingInfo"><view class="powerBox"><view class="title">充电功率</view><view class="power"><block wx:if="{{show}}"><view class="num">{{$root.g0}}</view></block><view class="unit">KW</view></view></view><view class="line"></view><view class="ampereBox"><view class="ampereBoxItem">{{chargeInfo.contents.chargingVolt}}<text style="font-size:24rpx;">V</text></view><view class="ampereBoxItem" style="margin-top:20rpx;">{{chargeInfo.contents.chargingAmpere}}<text style="font-size:24rpx;">A</text></view></view></view><view class="flex moneyBox flex-col-center" style="margin-top:108rpx;"><view class="title">充电时间</view><block wx:if="{{show}}"><view class="num">{{chargeInfo.contents.chargingedTime/60000}}</view></block><view class="unit">分钟</view></view><view class="flex moneyBox flex-col-center" style="margin-top:66rpx;"><view class="title">充电度数</view><block wx:if="{{show}}"><view class="num">{{chargeInfo.contents.chargingedKwh}}</view></block><view class="unit">度</view></view><view class="flex moneyBox flex-col-center" style="margin-top:66rpx;"><view class="title">充电费用</view><block wx:if="{{show}}"><view class="num">{{''+(chargeInfo.contents.chargingUseLog.chargingFee+chargeInfo.contents.chargingUseLog.serviceFee)+''}}</view></block><view class="unit">元</view></view><view data-event-opts="{{[['tap',[['stopCharge',['$event']]]]]}}" class="endChargingBox" bindtap="__e">结束充电</view></view>