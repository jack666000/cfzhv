page {
  background: #F5F7FF;
}
.payBox {
  width: 686rpx;
  height: 152rpx;
  background: #2B3041;
  border-radius: 75rpx;
  position: absolute;
  left: 32rpx;
  bottom: 84rpx;
}
.pay {
  width: 222rpx;
  height: 96rpx;
  background: linear-gradient(270deg, #F3DBA6 0%, #E2B46D 100%);
  border-radius: 48rpx;
  font-size: 32rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #5E3400;
  line-height: 96rpx;
  text-align: center;
  margin: 28rpx 44rpx 0 0;
}
.totalBox .left {
  font-size: 32rpx;
  font-weight: 400;
  color: #FFFFFF;
  margin: 46rpx 0 0 58rpx;
}
.totalBox .right {
  font-size: 52rpx;
  font-weight: 600;
  color: #EFB25A;
  margin: 26rpx 0 0 20rpx;
}
.discount {
  height: 34rpx;
  font-size: 24rpx;
  font-weight: 400;
  color: #FFFFFF;
  line-height: 34rpx;
  margin: 0rpx 0 0 58rpx;
  opacity: .7;
}
