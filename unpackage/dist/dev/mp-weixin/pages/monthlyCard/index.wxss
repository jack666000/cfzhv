page {
  background-color: #FFFFFF;
}
.parkBox {
  width: 686rpx;
  height: 76rpx;
  margin-left: 32rpx;
  margin-top: 40rpx;
}
.parkBox .left :nth-child(1) {
  width: 516rpx;
  height: 40rpx;
  font-size: 28rpx;
  font-weight: 400;
  color: #323233;
  line-height: 40rpx;
}
.parkBox .left :nth-child(2) {
  width: 516rpx;
  height: 32rpx;
  font-size: 22rpx;
  font-weight: 400;
  color: rgba(50, 50, 51, 0.5);
  line-height: 32rpx;
}
.parkBox .right {
  width: 138rpx;
  height: 64rpx;
}
.searchBox {
  width: 750rpx;
  height: 128rpx;
  background-color: #F5F7FF;
  overflow: hidden;
}
.searchBox .inputBox {
  width: 686rpx;
  height: 80rpx;
  background: #FFFFFF;
  border-radius: 40rpx;
  border: 1rpx solid #DADEEA;
  margin: 24rpx 0 0 32rpx;
}
.searchBox .inputBox image {
  width: 48rpx;
  height: 48rpx;
  margin-left: 34rpx;
}
.searchBox .inputBox input {
  height: 100%;
  margin-left: 16rpx;
}

