/* 水平间距 */
.listItem {
  width: 686rpx;
  background: #ffffff;
  border-radius: 10rpx;
  margin: 0 0 32rpx 32rpx;
  padding: 32rpx;
}
.listItem .name {
  height: 50rpx;
  font-size: 36rpx;
  font-weight: 600;
  color: #323233;
  line-height: 50rpx;
}
.listItem .address {
  margin-top: 8rpx;
}
.listItem .address view:nth-child(1) {
  width: 92rpx;
  height: 32rpx;
  line-height: 32rpx;
  background: rgba(78, 146, 246, 0.13);
  border-radius: 4rpx;
  font-size: 20rpx;
  font-weight: 400;
  color: #4e92f6;
  text-align: center;
}
.listItem .address view:nth-child(2) {
  width: 514rpx;
  height: 32rpx;
  font-size: 24rpx;
  font-weight: 400;
  color: #434343;
  line-height: 34rpx;
  margin-left: 16rpx;
}
.listItem .spaces {
  height: 32rpx;
  font-size: 24rpx;
  font-weight: 400;
  color: #323233;
  line-height: 32rpx;
  margin-top: 16rpx;
}

