/* 水平间距 */
page {
  background: #F8F9FB;
}
.popupBox {
  width: 686rpx;
  height: 684rpx;
  background: #FFFFFF;
  border-radius: 16rpx;
  margin-left: 32rpx;
  overflow: hidden;
  margin-bottom: 76rpx;
  padding: 0 34rpx;
}
.status {
  width: 750rpx;
  height: 100rpx;
  background: #FFFFFF;
  margin-top: 24rpx;
  justify-content: space-between;
}
.status .left {
  height: 44rpx;
  font-size: 32rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(50, 50, 51, 0.7);
  line-height: 44rpx;
  margin: 28rpx 0 0 32rpx;
}
.status .right {
  margin-right: 32rpx;
}
.status .right .one {
  width: 96rpx;
  height: 44rpx;
  font-size: 32rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(50, 50, 51, 0.5);
  line-height: 44rpx;
}
.status .right .two {
  height: 48rpx;
  font-size: 34rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #323233;
  line-height: 48rpx;
}
.status .right .image {
  width: 32rpx;
  height: 32rpx;
}
.takeapicture {
  width: 670rpx;
  height: 360rpx;
  background: #FFFFFF;
  border-radius: 18rpx;
  margin: 24rpx 0 0 40rpx;
}
.takeapicture image {
  width: 64rpx;
  height: 64rpx;
}
.takeapicture .text {
  width: 256rpx;
  height: 44rpx;
  font-size: 32rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #9B9C9E;
  line-height: 44rpx;
  margin-top: 12rpx;
}

