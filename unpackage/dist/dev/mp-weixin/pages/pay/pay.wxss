/* 水平间距 */
page {
  background: #F5F7FF;
  padding-bottom: 300rpx;
}
.payBox {
  width: 686rpx;
  height: 152rpx;
  background: #2B3041;
  border-radius: 75rpx;
  position: fixed;
  left: 32rpx;
  bottom: 84rpx;
}
.pay {
  width: 222rpx;
  height: 96rpx;
  background: linear-gradient(270deg, #F3DBA6 0%, #E2B46D 100%);
  border-radius: 48rpx;
  font-size: 32rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #5E3400;
  line-height: 96rpx;
  text-align: center;
  margin: 28rpx 44rpx 0 0;
}
.totalBox .left {
  font-size: 32rpx;
  font-weight: 400;
  color: #FFFFFF;
  margin: 46rpx 0 0 58rpx;
}
.totalBox .right {
  font-size: 52rpx;
  font-weight: 600;
  color: #EFB25A;
  margin: 26rpx 0 0 20rpx;
}
.discount {
  height: 34rpx;
  font-size: 24rpx;
  font-weight: 400;
  color: #FFFFFF;
  line-height: 34rpx;
  margin: 0rpx 0 0 58rpx;
  opacity: .7;
}
.licensePlateBox {
  width: 698rpx;
  height: 160rpx;
  background: #FFFFFF;
  border-radius: 10rpx;
  margin: 20rpx 0 0 26rpx;
}
.licensePlateBox .left {
  width: 96rpx;
  font-size: 32rpx;
  font-weight: 400;
  color: rgba(50, 50, 51, 0.6);
  margin-left: 32rpx;
}
.licensePlateBox .right {
  width: 502rpx;
  font-size: 32rpx;
  font-weight: 400;
  color: #323233;
  margin-left: 36rpx;
  text-align: right;
}
.parkNameBox {
  width: 698rpx;
  height: 100rpx;
  background: #FFFFFF;
  border-radius: 10rpx;
  margin: 32rpx 0 0 26rpx;
}
.parkNameBox .left {
  width: 96rpx;
  height: 44rpx;
  font-size: 32rpx;
  font-weight: 400;
  color: rgba(50, 50, 51, 0.6);
  line-height: 44rpx;
  margin-left: 32rpx;
}
.parkNameBox .right {
  width: 502rpx;
  height: 44rpx;
  font-size: 32rpx;
  font-weight: 400;
  color: #323233;
  line-height: 44rpx;
  margin-left: 36rpx;
  text-align: right;
}
.choose {
  width: 750rpx;
  background: #FFFFFF;
  border-radius: 48rpx 48rpx 0rpx 0rpx;
  align-self: flex-end;
}
.choose .title {
  height: 60rpx;
  font-size: 44rpx;
  font-family: PingFangSC-Semibold, PingFang SC;
  font-weight: 600;
  color: #323233;
  line-height: 60rpx;
  margin: 40rpx 0 0 40rpx;
}
.choose .subtitle {
  height: 34rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #323233;
  line-height: 34rpx;
  margin: 4rpx 0 0 40rpx;
}
.choose .licensePlate {
  background: #F8F9FB;
  border-radius: 12rpx;
  border: 2rpx solid rgba(151, 151, 151, 0.08);
  width: 670rpx;
  height: 136rpx;
  border-radius: 12rpx;
  font-size: 44rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #323233;
  line-height: 136rpx;
  padding-left: 32rpx;
  margin-left: 42rpx;
  margin-top: 40rpx;
}
.choose .licensePlate .title1 {
  height: 60rpx;
  font-size: 44rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #323233;
  line-height: 60rpx;
  margin-top: 20rpx;
}
.choose .licensePlate .subTitle1 {
  width: 284rpx;
  height: 32rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #323233;
  line-height: 32rpx;
  margin-top: 4rpx;
}
.choose .addCarBox {
  width: 670rpx;
  height: 136rpx;
  background: rgba(72, 140, 245, 0.08);
  border-radius: 12rpx;
  border: 3rpx dashed #488CF5;
  margin: 46rpx 0 0 40rpx;
  margin-bottom: 32rpx;
  margin-top: 20rpx;
}
.choose .addCarBox image {
  width: 32rpx;
  height: 32rpx;
}
.choose .addCarBox .text {
  width: 144rpx;
  height: 50rpx;
  font-size: 36rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #488CF5;
  line-height: 50rpx;
}

